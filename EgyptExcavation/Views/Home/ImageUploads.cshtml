@{
}
@model IEnumerable<Files> 

@foreach (var img in Model)
{
    @if (img.FileType.Contains("jpg") || img.FileType.Contains("png") || img.FileType.Contains("JPG") || img.FileType.Contains("PNG"))
    {
                    <img src="@img.GetImageFromByteArray()" />
                }
}